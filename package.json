{
  "name": "muze-monorepo",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "6.1.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.11",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdoc": "3.4.3",
    "karma": "1.7.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^3.0.4",
    "lerna": "^3.0.4",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "3.4.2",
    "mocha-webpack": "0.7.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "transform-runtime": "0.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "test": "npm run lint && karma start karma.conf.js",
    "utd": "karma start karma.conf.js --browsers=Chrome --single-run=false --debug --auto-watch",
    "ut": "karma start karma.conf.js",
    "clean": "rimraf ./dist",
    "copy-dist": "cp -R dist packages/muze/npm/",
    "copy-rdm": "cp README.md packages/muze/ && cp README.md packages/muze/npm/",
    "build:prod": "npm run clean && webpack --mode production && npm run copy-dist && npm run copy-rdm",
    "build:dev": "npm run clean && webpack --mode development && npm run copy-dist && npm run copy-rdm",
    "build": "npm run build:prod",
    "start:prod": "webpack-dev-server --host 0.0.0.0 --open --mode development --config webpack.config.js",
    "start:dev": "webpack-dev-server --host 0.0.0.0 --open --mode development --config webpack.config.dev.js",
    "start": "npm run start:dev",
    "lint": "eslint ./packages",
    "lint-errors": "eslint --quiet ./packages",
    "docs": "jsdoc -c ./jsdoc.conf.json",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {},
  "browserslist": [
    "last 10 major versions",
    "not dead",
    "Firefox ESR",
    "ie >= 11"
  ]
}
